Price_Download <- function(ticker, price_type = 1, from, to) {
  Price <- lapply(ticker, function(i) {(getSymbols(i, from = from, to = to, auto.assign=FALSE, src=)[, price_type])})
  Price<- do.call(merge, Price)
  Price<-na.omit(Price)
  return(Price)
}

Price_Download("^DJI", 5, "2019-05-01", "2019-05-10")

MyStocks <- c("GE", "FORD", "BA", "^DJI", "DB")
Price_Download(MyStocks, 4, from = "2019-05-01", to = "2019-05-10")
